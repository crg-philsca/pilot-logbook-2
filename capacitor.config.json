{
  "appId": "com.pilotlogbook.app",
  "appName": "Pilot Logbook",
  "webDir": "dist",
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 0,
      "launchAutoHide": true,
      "backgroundColor": "#020617",
      "androidSplashResourceName": "splash",
      "androidScaleType": "CENTER_CROP",
